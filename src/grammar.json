{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "opam",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_item"
      }
    },
    "_item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "SYMBOL",
          "name": "section"
        }
      ]
    },
    "variable": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "ident"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "value"
          }
        }
      ]
    },
    "section": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "kind",
          "content": {
            "type": "SYMBOL",
            "name": "ident"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "name",
              "content": {
                "type": "SYMBOL",
                "name": "string"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "FIELD",
          "name": "items",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_item"
            }
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "atom"
        },
        {
          "type": "SYMBOL",
          "name": "group"
        },
        {
          "type": "SYMBOL",
          "name": "list"
        },
        {
          "type": "SYMBOL",
          "name": "option_value"
        },
        {
          "type": "SYMBOL",
          "name": "logop_value"
        },
        {
          "type": "SYMBOL",
          "name": "relop_value"
        },
        {
          "type": "SYMBOL",
          "name": "env_binding"
        },
        {
          "type": "SYMBOL",
          "name": "prefix_relop"
        },
        {
          "type": "SYMBOL",
          "name": "pfxop_value"
        }
      ]
    },
    "atom": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "ident"
        },
        {
          "type": "SYMBOL",
          "name": "bool"
        },
        {
          "type": "SYMBOL",
          "name": "int"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "group": {
      "type": "PREC",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "("
          },
          {
            "type": "FIELD",
            "name": "values",
            "content": {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "value"
              }
            }
          },
          {
            "type": "STRING",
            "value": ")"
          }
        ]
      }
    },
    "list": {
      "type": "PREC",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "["
          },
          {
            "type": "FIELD",
            "name": "values",
            "content": {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "value"
              }
            }
          },
          {
            "type": "STRING",
            "value": "]"
          }
        ]
      }
    },
    "option_value": {
      "type": "PREC_RIGHT",
      "value": 8,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "value",
            "content": {
              "type": "SYMBOL",
              "name": "value"
            }
          },
          {
            "type": "STRING",
            "value": "{"
          },
          {
            "type": "FIELD",
            "name": "options",
            "content": {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "value"
              }
            }
          },
          {
            "type": "STRING",
            "value": "}"
          }
        ]
      }
    },
    "logop_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC_LEFT",
          "value": 6,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "FIELD",
                "name": "left",
                "content": {
                  "type": "SYMBOL",
                  "name": "value"
                }
              },
              {
                "type": "STRING",
                "value": "&"
              },
              {
                "type": "FIELD",
                "name": "right",
                "content": {
                  "type": "SYMBOL",
                  "name": "value"
                }
              }
            ]
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 7,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "FIELD",
                "name": "left",
                "content": {
                  "type": "SYMBOL",
                  "name": "value"
                }
              },
              {
                "type": "STRING",
                "value": "|"
              },
              {
                "type": "FIELD",
                "name": "right",
                "content": {
                  "type": "SYMBOL",
                  "name": "value"
                }
              }
            ]
          }
        }
      ]
    },
    "relop_value": {
      "type": "PREC",
      "value": 4,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "left",
            "content": {
              "type": "SYMBOL",
              "name": "atom"
            }
          },
          {
            "type": "FIELD",
            "name": "operator",
            "content": {
              "type": "SYMBOL",
              "name": "relop"
            }
          },
          {
            "type": "FIELD",
            "name": "right",
            "content": {
              "type": "SYMBOL",
              "name": "atom"
            }
          }
        ]
      }
    },
    "env_binding": {
      "type": "PREC",
      "value": 5,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "variable",
            "content": {
              "type": "SYMBOL",
              "name": "atom"
            }
          },
          {
            "type": "FIELD",
            "name": "operator",
            "content": {
              "type": "SYMBOL",
              "name": "envop"
            }
          },
          {
            "type": "FIELD",
            "name": "value",
            "content": {
              "type": "SYMBOL",
              "name": "atom"
            }
          }
        ]
      }
    },
    "prefix_relop": {
      "type": "PREC",
      "value": 4,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "operator",
            "content": {
              "type": "SYMBOL",
              "name": "relop"
            }
          },
          {
            "type": "FIELD",
            "name": "value",
            "content": {
              "type": "SYMBOL",
              "name": "atom"
            }
          }
        ]
      }
    },
    "pfxop_value": {
      "type": "PREC",
      "value": 3,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "operator",
            "content": {
              "type": "SYMBOL",
              "name": "pfxop"
            }
          },
          {
            "type": "FIELD",
            "name": "value",
            "content": {
              "type": "SYMBOL",
              "name": "value"
            }
          }
        ]
      }
    },
    "relop": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "STRING",
          "value": "!="
        },
        {
          "type": "STRING",
          "value": ">="
        },
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "STRING",
          "value": "<="
        },
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "STRING",
          "value": "~"
        }
      ]
    },
    "pfxop": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "!"
        },
        {
          "type": "STRING",
          "value": "?"
        }
      ]
    },
    "envop": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "STRING",
          "value": "+="
        },
        {
          "type": "STRING",
          "value": "=+"
        },
        {
          "type": "STRING",
          "value": ":="
        },
        {
          "type": "STRING",
          "value": "=:"
        },
        {
          "type": "STRING",
          "value": "=+="
        }
      ]
    },
    "bool": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "int": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "-"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "PATTERN",
            "value": "[0-9_]+"
          }
        ]
      }
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "_string_content"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "escape_sequence"
                  }
                ]
              }
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\"\"\""
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "REPEAT",
                "content": {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "_string_content_triple"
                    },
                    {
                      "type": "SYMBOL",
                      "name": "escape_sequence"
                    }
                  ]
                }
              },
              "named": false,
              "value": "string_content"
            },
            {
              "type": "STRING",
              "value": "\"\"\""
            }
          ]
        }
      ]
    },
    "_string_content": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "[^\"\\\\]+"
        }
      }
    },
    "_string_content_triple": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[^\"\\\\]+"
            },
            {
              "type": "PATTERN",
              "value": "\"[^\"]"
            },
            {
              "type": "PATTERN",
              "value": "\"\"[^\"]"
            }
          ]
        }
      }
    },
    "escape_sequence": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\\"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[\\\\'\"nrtb ]"
                },
                {
                  "type": "PATTERN",
                  "value": "[0-9]{3}"
                },
                {
                  "type": "PATTERN",
                  "value": "x[0-9a-fA-F]{2}"
                },
                {
                  "type": "PATTERN",
                  "value": "\\r?\\n[ \\t]*"
                }
              ]
            }
          ]
        }
      }
    },
    "ident": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[a-zA-Z0-9_-]*[a-zA-Z][a-zA-Z0-9_-]*"
              },
              {
                "type": "STRING",
                "value": "_"
              }
            ]
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "+"
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "PATTERN",
                      "value": "[a-zA-Z0-9_-]*[a-zA-Z][a-zA-Z0-9_-]*"
                    },
                    {
                      "type": "STRING",
                      "value": "_"
                    }
                  ]
                }
              ]
            }
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": ":"
                  },
                  {
                    "type": "PATTERN",
                    "value": "[a-zA-Z0-9_-]*[a-zA-Z][a-zA-Z0-9_-]*"
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "PREC",
            "value": 0,
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "#"
                },
                {
                  "type": "PATTERN",
                  "value": "[^\\n]*"
                }
              ]
            }
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "PREC",
            "value": 0,
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "(*"
                },
                {
                  "type": "PATTERN",
                  "value": "[^*]*\\*+(?:[^)*][^*]*\\*+)*"
                },
                {
                  "type": "STRING",
                  "value": ")"
                }
              ]
            }
          }
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\s+"
    }
  ],
  "conflicts": [
    [
      "relop",
      "envop"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}